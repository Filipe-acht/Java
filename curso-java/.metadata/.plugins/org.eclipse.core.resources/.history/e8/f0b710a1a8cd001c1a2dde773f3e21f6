package fundamentos.operadores;

public class Unarios {

	public static void main(String[] args) {
		
		int a = 1;
		int b = 2;
		
		//posfix
		a++; // (a = a + 1) - Incrementando ao valor da variavel
		a--; // (a = a - 1) - Decrementando
		
		//prefix
		++b;
		--b;
		
		System.out.println(a);
		System.out.println(b);
	}
}
