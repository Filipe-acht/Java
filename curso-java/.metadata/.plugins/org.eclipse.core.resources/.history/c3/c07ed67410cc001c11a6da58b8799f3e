package fundamentos;

public class DesafioAritmeticos {

	public static void main(String[] args) {
		
		double eq1 = 6 * (3 + 2);
		double eq1_1 = Math.pow(eq1, 2) / (3 * 2);
		
		double eq2 = ((1 - 5) * (2 - 7) ) / 2;
		
		//System.out.println(eq1);
		//System.out.println(eq1_1);
		System.out.println(eq2);
	}
}
