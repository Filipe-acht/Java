package fundamentos;

public class DesafioAritmeticos {

	public static void main(String[] args) {
		
		double eq1 = 6 * (3 + 2);
		double eq1_1 = Math.pow(eq1, 2);
		double eq1_1_2 = Math.pow(eq1, 2) / (3 * 2);
		
		System.out.println(eq1);
		System.out.println(eq1_1);
		System.out.println(eq1_1_2);
	}
}
