package fundamentos;

public class Temperatura {

	// (ºF - 32) x 5/9(no java 5/9.0 ou 5.0/9.0 ou 5.0/9) = ºC
	public static void main(String[] args) {
		
		final int C1 = 32;
		final double C2 = 5.0/9.0;
		double temp_farenh = 145.5;
		
		//(145,5 °F − 32) × 5/9 = 790,556 °C

		double temp_celsius = ((temp_farenh - C1) * C2);
		
		System.out.println(temp_celsius);
		
	}
}
