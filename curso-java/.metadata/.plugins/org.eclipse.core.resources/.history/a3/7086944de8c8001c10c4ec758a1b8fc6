package fundamentos;

public class ConversaoTipoPrimitivoNumerico {
	
	public static void main(String[] args) {
		
		double a = 1; //  conversão implicita de um int para um double
		System.out.println(a);
		
		//float b = 1.0F; // F - para tornar um literal num float
		
		// Conversão usando CAST
		float b  = (float) 1.12345;// converta para float e armazene numa variavel float
		System.out.println(b);
		
		
	}

}
